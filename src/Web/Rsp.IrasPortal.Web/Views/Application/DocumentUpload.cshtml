@{
    ViewData["Title"] = "Application";
}

<partial name="_BackNavigation" model="@("app:projectstartdate","Back")" />

<div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">
        <h1 class="govuk-heading-xl" id="title">Upload any supporting documents</h1>
    </div>
</div>
<form method="post">
    <div class="govuk-form-group">
        <input class="govuk-file-upload" id="supportingDocument" name="supportingDocument" type="file" />
    </div>

    <partial name="_SubmitButton" model="@("app:savedocumentupload","Review your answers")" />

    @{
        ViewBag.Style = "ms-3 govuk-button govuk-button--secondary";
    }

    <partial name="_SubmitButton" model="@("app:savedraftapplication","Save draft")" />
</form>